# Code Coverage Configuration

[run]
source = .
omit = 
    tests/*
    .venv/*
    __pycache__/*
    setup.py
    docs/*
    .githooks/*
    packaging/*
    installers/*
    .vscode-extension/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract

precision = 2
show_missing = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
